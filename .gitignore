# Ignore bundler config.
/.bundle
vendor/bundle/*

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/node_modules
/yarn-error.log

node_modules/
bundle.js
bundle.js.map
.byebug_history
.DS_Store
npm-debug.log

*.mp3

# Ignore application configuration
/config/application.yml

# Ignore seed user deletion script
delete_seed_users.txt

# Ignore seed songs folder
*/seed_songs/*

# auto-gen license file
bundle.js.LICENSE.txt